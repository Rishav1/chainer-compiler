{
    "simplify_preproc": {
        "ReplaceChainerSelectItem": true,
        "ReplaceChainerSoftmaxCrossEntropy": true,
        "ReplaceIdentity": true,
        // TODO(hamaji): Revive Scan.
        // "ReplaceScan": true,
        "ReplaceShape": true
    },
    "simplify": {
        "ReplaceArgMin": true,
        "ReplaceConstantLike": true,
        "ReplaceConstantOfShape": true,
        "ReplaceConv": true,
        "ReplaceFlatten": true,
        "ReplaceGlobalAveragePool": true,
        "ReplaceGlobalMaxPool": true,
        "ReplaceImageScaler": true,
        "ReplaceLess": true,
        "ReplaceLpNormalization": true,
        "ReplaceMaxRoiPool": true,
        "ReplaceMean": true,
        "ReplaceReduceL1": true,
        "ReplaceReduceL2": true,
        "ReplaceReduceLogSum": true,
        "ReplaceReduceLogSumExp": true,
        "ReplaceReduceMin": true,
        "ReplaceSlice": true,
        "ReplaceSoftplus": true,
        "ReplaceSoftsign": true,
        "ReplaceSum": true,
        // These passes are workarounds for Chainer which does not
        // support pooling with imbalanced padding.
        "ReplaceMaxPool": true,
        "ReplaceAveragePool": true,
        // Backprop only.
        "ReplaceConcat": true
    }
}
